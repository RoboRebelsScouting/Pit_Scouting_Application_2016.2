            android:text="                          Notes"
            android:id="@+id/textView23"
            android:layout_column="1" />

        <EditText
            android:layout_width="6dp"
            android:layout_height="wrap_content"
            android:id="@+id/Notes1"
            android:inputType= "textPersonName"
            android:layout_column="2" />

    </TableRow>
